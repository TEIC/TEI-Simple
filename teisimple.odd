                        </valItem>
                        <valItem ident="simple:rotateleft">
                           <skos:exactMatch>rotateCounterclockwise</skos:exactMatch>
                        </valItem>
                        <valItem ident="simple:centre">
                           <skos:exactMatch>center</skos:exactMatch>
                        </valItem>
                        <valItem ident="simple:braced "/>
